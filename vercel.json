{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "font.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "index.html" }]
}
